{
  "name": "todo-note",
  "displayName": "Todo Note",
  "version": "0.0.1",
  "publisher": "ch-kim",
  "engines": {
    "vscode": "^1.96.0"
  },
  "main": "./src/cjs-wrapper.cjs",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "todo-note-explorer",
          "title": "Todo Note",
          "icon": "$(star)"
        }
      ]
    },
    "views": {
      "todo-note-explorer": [
        {
          "id": "todo-note-view",
          "name": "Todo Note"
        }
      ]
    },
    "commands": [
      {
        "command": "todo-note.addNote",
        "title": "Add note"
      }
    ]
  },
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "todo-note": "file:"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0"
  }
}
